{
  "name": "ts",
  "version": "1.0.0",
  "description": "testing ts and sequelize",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/server.js",
    "build": "npm run build-ts && npm run tslint && npm run cp-assets",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\"  \"npm run watch-ts\" \"npm run watch-node\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "clean": "rm -r dist/*",
    "rebuild": "rm -r dist/* && npm run build",
    "compile-mig": "tsc --p mig-tsconfig.json",
    "clean-mig": "rm -r migrations/compiled/*",
    "recompile-mig": "npm run clean-mig && npm run compile-mig",
    "compile-seed": "tsc seeders/*.ts",
    "clean-seed": "rm seeders/*.js",
    "recompile-seed": "npm run clean-seed && npm run compile-seed",
    "cp-assets": "ts-node copyStaticAssets.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "fecha": "^2.3.2",
    "graphql": "^0.12.3",
    "graphql-tools": "^2.19.0",
    "javascript-time-ago": "^1.0.23",
    "pg": "^6.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.4",
    "sequelize-typescript": "^0.6.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.0",
    "@types/fecha": "^2.3.1",
    "@types/graphql": "^0.12.3",
    "@types/pg": "^6.1.45",
    "@types/umzug": "^2.1.0",
    "reflect-metadata": "^0.1.12",
    "shelljs": "^0.8.1",
    "typescript": "^2.6.2"
  }
}
